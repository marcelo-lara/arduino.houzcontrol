#include <DMXSerial.h>

void setup() 
{
//   Serial.begin(115200);
//   Serial.println("--start");

  for (int i = 8; i < 14; i++)
  {
	  pinMode(i, INPUT_PULLUP);
  }
  DMXSerial.init(DMXController,2);


}
int incomingByte = 0;
int upval=254;
unsigned long nextPrint;
void loop()
{
  int ch;
  for (int i = 8; i < 14; i++)
  {
    int val=digitalRead(i)==0?0:upval;
    DMXSerial.write(ch, val);
    ch++;
  }
  delayMicroseconds(2000); // wait a little bit

}
